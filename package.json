{
  "name": "Ride-My-Way",
  "version": "1.0.0",
  "description": "Ride-my-Way API is a carpooling service API built to provides users with the ability to create ride offers and join available ride offers.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./lib/index.js",
    "postinstall": "npm run build",
    "start": "babel-node dist/index.js",
    "dev": "npm run clean && babel-node ./lib/index.js --presets env",
    "test": "mocha ./lib/test/ --recursive --compilers js:babel-register --exit",
    "build": "npm run clean && babel lib -d dist --presets env",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "carpooling",
    "api",
    "ride",
    "ride-my-way",
    "my way",
    "ride my way"
  ],
  "author": "Babatunde Adelabu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "dotenv": "^6.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "aglio": "^2.3.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.0",
    "pg": "^7.4.3"
  },
  "engines": {
    "node": "^4.7.0",
    "npm": "^5.5.1"
  }
}
